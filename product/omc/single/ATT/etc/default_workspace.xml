<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">

    <home>
    <homeGridInfo default="4x5" />
        <!-- appwidget : Weather -->
        <appwidget 
            screen="0" 
            packageName="com.sec.android.daemonapp" 
            className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1" 
            x="0" 
            y="2" 
            spanX="4" 
            spanY="1" />

        <!-- appwidget : Google -->
        <appwidget 
            screen="0" 
            packageName="com.google.android.googlequicksearchbox" 
            className="com.google.android.googlequicksearchbox.SearchWidgetProvider" 
            x="0" 
            y="3" 
            spanX="4" 
            spanY="1" />

        <!-- Galaxy Store -->
        <favorite 
            screen="0" 
            packageName="com.sec.android.app.samsungapps" 
            className="com.sec.android.app.samsungapps.SamsungAppsMainActivity" 
            x="0" 
            y="4" />

        <!-- Gallery -->
        <favorite 
            screen="0" 
            packageName="com.sec.android.gallery3d" 
            className="com.samsung.android.gallery.app.activity.GalleryActivity" 
            x="1" 
            y="4" />

        <!-- Google Play Store -->
        <favorite 
            screen="0" 
            packageName="com.android.vending" 
            className="com.android.vending.AssetBrowserActivity" 
            x="2" 
            y="4" />

        <!-- folder : Google -->
        <folder 
            screen="0" 
            title="Google" 
            x="3" 
            y="4">
            <!-- Google -->
            <favorite 
                screen="0" 
                packageName="com.google.android.googlequicksearchbox" 
                className="com.google.android.googlequicksearchbox.SearchActivity" />
            <!-- Gmail -->
            <favorite 
                screen="1" 
                packageName="com.google.android.gm" 
                className="com.google.android.gm.ConversationListActivityGmail" />
            <!-- Maps -->
            <favorite 
                screen="2" 
                packageName="com.google.android.apps.maps" 
                className="com.google.android.maps.MapsActivity" />
            <!-- YouTube -->
            <favorite 
                screen="3" 
                packageName="com.google.android.youtube" 
                className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" />
            <!-- Drive -->
            <favorite 
                screen="4" 
                packageName="com.google.android.apps.docs" 
                className="com.google.android.apps.docs.app.NewMainProxyActivity" />
            <!-- Google Play Music -->
            <favorite 
                screen="5" 
                packageName="com.google.android.music" 
                className="com.android.music.activitymanagement.TopLevelActivity" />
            <!-- Google Play Movies & TV -->
            <favorite 
                screen="6" 
                packageName="com.google.android.videos" 
                className="com.google.android.youtube.videos.EntryPoint" />
            <!-- Duo -->
            <favorite 
                screen="7" 
                packageName="com.google.android.apps.tachyon" 
                className="com.google.android.apps.tachyon.MainActivity" />
            <!-- Photos -->
            <favorite 
                screen="8" 
                packageName="com.google.android.apps.photos" 
                className="com.google.android.apps.photos.home.HomeActivity" />
        </folder>

        <!-- Games -->
        <favorite 
            screen="1" 
            packageName="com.dti.folderlauncher" 
            className="com.dti.folderlauncher.MainActivity" 
    	      postPosition="true"
            x="0" 
            y="3" />
    			
        <!-- folder : Video -->
    		<folder 
            screen="1" 
            title="Video"
            postPosition="true"
            x="1"
            y="3" >
            <!-- HBO GO -->
            <favorite 
                screen="1"
                packageName="com.HBO" 
                className="com.hbo.go.LaunchActivity" />
            <!-- DC Universe -->
            <favorite 
                screen="2" 
                packageName="com.wb.goog.dcuniverse"
                className="com.dccomics.universe.activity.LaunchActivity" />
            <!-- Bleacher Report Live -->
            <favorite 
                screen="3" 
                packageName="com.catchsports.catchsports" 
                className="com.catchsports.catchsports.ui.splash.SplashActivity" />
    		</folder>
    
        <!-- folder : News & Sports -->
        <folder 
            screen="1" 
            title="News &amp; Sports"
            postPosition="true"
            x="2"
            y="3" >
         		<!-- CNN -->
            <favorite 
                screen="0" 
                packageName="com.cnn.mobile.android.phone" 
                className="com.cnn.mobile.android.phone.features.splash.SplashActivity" />
            <!-- Great Big Story-->
            <favorite 
                screen="1" 
                packageName="com.greatbigstory.greatbigstory" 
                className="com.greatbigstory.greatbigstory.app.ui.launch.LaunchActivity" />	
        </folder>
    
        <!-- folder : AT&T -->
        <folder 
            screen="1" 
            title="AT&amp;T"
            postPosition="true"
            x="3"
            y="3" >
			
			<!-- AT&T Personal Cloud -->
			<favorite 
            screen="0" 
            packageName="com.att.personalcloud" 
            className="com.att.personalcloud.gui.activities.SplashLogoActivity" />
    
            <!-- AT&T Call Protect -->
            <favorite 
                screen="1" 
                packageName="com.att.callprotect" 
                className="com.hiya.aegis.ui.HomeScreenActivity" />
            <!-- Device Help -->
            <favorite 
                screen="2"
                packageName="com.att.dh" 
                className="com.mce.MainActivity" />      
            <!-- Mobile Security -->
            <favorite 
                screen="3"
                packageName="com.att.mobilesecurity" 
                className="com.lookout.appcoreui.ui.launcher.LoadDispatchActivity" />
            <!-- Setup & Transfer -->
            <favorite 
                screen="4" 
                packageName="com.synchronoss.dcs.att.r2g" 
                className="com.synchronoss.dcs.att.r2g.ATTSplash" />
            <!-- AT&T Smart Wi-Fi -->
            <favorite 
                screen="5"
                packageName="com.att.android.attsmartwifi" 
                className="com.att.android.attsmartwifi.ui.SplashScreen" />
            <!-- Drive Mode -->
            <favorite 
                screen="6" 
                packageName="com.drivemode" 
                className="com.drivemode.activity.SplashScreen" />
            <!-- AT&T Secure Family -->
            <favorite 
                screen="7" 
                packageName="com.wavemarket.waplauncher" 
                className="com.locationlabs.locator.presentation.splash.SplashActivity" />
            <!-- AT&T Protech -->
            <favorite 
                screen="8"
                packageName="com.asurion.android.protech.att" 
                className="com.asurion.android.attprotech.activities.MainActivity" />
           
        </folder>
    		
        <!-- Game of Thrones -->
        <favorite 
            screen="1" 
            packageName="com.wb.goog.got.conquest" 
            className="com.google.firebase.MessagingUnityPlayerActivity"
    			  postPosition="true"
    			  x="0" 
            y="4" />
    
        <!-- DTV Now -->
        <favorite 
            screen="1" 
            packageName="com.att.tv" 
            className="com.att.mobile.dfw.activities.StartupActivity"
            x="1" 
            y="4" />
    
    		<!-- Bleacher Report -->
        <favorite 
            screen="1" 
            packageName="com.bleacherreport.android.teamstream" 
            className="com.bleacherreport.android.teamstream.TsLaunchActivity"
    			  postPosition="true"
            x="2" 
            y="4" />
    
    		<!-- myAT&T -->
        <favorite 
            screen="1" 
            packageName="com.att.myWireless" 
            className="com.att.myWireless.activity.login.SplashScreenActivity"
            x="3" 
            y="4" />
    </home>

    <hotseat>
        <!-- Phone -->
        <favorite 
            screen="0" 
            packageName="com.samsung.android.dialer" 
            className="com.samsung.android.dialer.DialtactsActivity" />

        <!-- Messages -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.messaging" 
            className="com.android.mms.ui.ConversationComposer" />

        <!-- Chrome -->
        <favorite 
            screen="2" 
            packageName="com.android.chrome" 
            className="com.google.android.apps.chrome.Main" />

        <!-- Camera -->
        <favorite 
            screen="3" 
            packageName="com.sec.android.app.camera" 
            className="com.sec.android.app.camera.Camera" />
    </hotseat>

